<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views
  http://axelor.com/xml/ns/object-views/object-views_5.4.xsd"> 
  
    <grid model="com.axelor.address.db.Address" title="StudentRecord" name="address-grid">
    <field name="blockNo" />
     <field name="roomNo" />
      <field name="buildName" />
       <field name="society" />
        <field name="pinCode" />
        <field name="village" />
        <field name="state" />
        <field name="school" />
    </grid>
  
   <form model="com.axelor.address.db.Address" title="Address" name="address-form">
    <panel>
      <field name="blockNo" />
     <field name="roomNo" />
      <field name="buildName" />
       <field name="society" />
        <field name="pinCode" />
        <field name="village" />
        <field name="state" />
          <field name="school" />
    </panel>
    </form>
  </object-views>