<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models
	http://axelor.com/xml/ns/domain-models/domain-models_5.4.xsd">


<module name="student" package="com.axelor.student.db"/>

<entity name="Student" lang="java">
	<string name="firstName" />
	<string name="lastName" />
	<string name="city"  translatable="true" />
	<string name="phoneNumber" />
	<string name="state" />
	<integer name="zip" />
	<decimal name="price" precision="8" scale="2"/>
	<binary name="photo" image="true" />
	<string name="email" encrypted="true" />
	</entity>	
</domain-models>
