<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/object-views
  http://axelor.com/xml/ns/object-views/object-views_5.4.xsd">


    <grid model="com.axelor.auth.db.Register" title="User Record"
		name="register-grid">
        <field name="fullName" />
		<field name="phoneNumber" />
		<field name="email" />
		<field name="address" />
		<field name="state" />
		<field name="image" />
	</grid>
	

	<form  model="com.axelor.auth.db.Register" title="User Record"
		name="register-form">
	
	<panel name="register" title="Register">
	<field name="fName" />
		<field name="mName" />
		<field name="lName" />
		<field name="phoneNumber" />
		<field name="email" />
		<field name="address" />
		<field name="state" />
		<field name="image" />
		<field name="password" />
	</panel>
		
	</form>


  </object-views>